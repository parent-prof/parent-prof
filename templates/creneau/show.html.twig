{% block title %}Creneau{% endblock %}

{% block body %}
    <h1>Creneau</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ creneau.id }}</td>
            </tr>
            <tr>
                <th>Heure_debut</th>
                <td>{{ creneau.heureDebut ? creneau.heureDebut|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Heure_fin</th>
                <td>{{ creneau.heureFin ? creneau.heureFin|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Occupe</th>
                <td>{{ creneau.occupe ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('creneau_index') }}">back to list</a>

    <a href="{{ path('creneau_edit', {'id': creneau.id}) }}">edit</a>

    {{ include('creneau/_delete_form.html.twig') }}
